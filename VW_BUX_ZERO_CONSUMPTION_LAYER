---- COLUMNS NEEDED FROM ORDERS
SELECT ORD.ORDERS_ORDER_ID, ORD. ORDERS_EVENT_ORDER_TYPE, ORD.ORDERS_QUANTITY, ORD.SECURITY_ID, ORD.SOR_ALGORITHM, ORD.ORDERS_SOURCE, ORD.TRADING_ACCOUNT_ID,
ORD.USER_HOME_CURRENCY, ORD.ORDERS_USER_ID, ORD.TIMESTAMP, ORD.ORDERS_ENTITY_TYPE, ORD.ORDER_TYPE, ORD.FEE_ESTIMATE_AMOUNT, ORD.ORDERS_CURRENCY, ORD.ORDERS_LIMIT_PRICE_AMOUNT,
ORD.LIMIT_PRICE_CURRENCY,
--- COLUMNS NEEDED FROM MASTERDATA
MAS.ALLOWED_ORDER_TYPES, MAS.CATEGORY, MAS.MASTERDATA_CURRENCY, MAS.CUSTODIAN, MAS.EXECUTING_BROKER, MAS.MASTERDATA_INDEX, MAS.ISIN, MAS.MASTERDATA_NAME, MAS.SECURITY_STATUS,
MAS.SOR_MODEL, MAS.TAGS, MAS.MASTERDATA_EVENT_TYPE, MAS.MASTERDATA_STREAMINFO_ID,
--- COLUMNS NEEDED FROM ACCOUNTS
ACC.BUX_USERACCOUNT_NUMBER, ACC.BUX_USERACCOUNT_TYPE, ACC.ACCOUNTS_CURRENCY, ACC.CITY, ACC.COUNTRY, ACC.LANGUAGE, ACC.CITY_OF_BIRTH, ACC.COUNTRY_OF_BIRTH, ACC.GENDER, ACC.NATIONALITY,
ACC.RISK_SCORE, ACC.SUBSCRIPTION_TYPE, ACC.ACCOUNTS_USER_ID, ACC.ACCOUNT_ENTITY_TYPE
FROM EXT_ANALYTICS_ENGINEER_HKUMAR.HKUMAR.VW_BROKER_ORDERS_DOMAINEVENTS_SOURCE ORD
INNER JOIN EXT_ANALYTICS_ENGINEER_HKUMAR.HKUMAR.VW_BROKER_MASTERDATA_DOMAINEVENTS_SOURCE MAS
ON MAS.ISIN = ORD.SECURITY_ID
INNER JOIN EXT_ANALYTICS_ENGINEER_HKUMAR.HKUMAR.VW_BROKER_ACCOUNTS_DOMAINEVENTS_SOURCE ACC
on ORD.ORDERS_USER_ID = ACC.ACCOUNTS_USER_ID